{
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "api/index.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}